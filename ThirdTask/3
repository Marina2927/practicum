#include <iostream>

using namespace std;

int main(){
    setlocale(LC_CTYPE, "rus");
    int xm, ym, xo, yo, r;
    cout << "Введите координыты точки М" << endl;
    cin >> xm >> ym;
    cout << "Введите координыты центра окружности" << endl;
    cin >> xo >> yo;
    cout << "Введите длину радиуса" << endl;
    cin >> r;

    if ((xo-xm)*(xo-xm)+(yo-ym)*(yo-ym)<=r*r){
        cout << "Лежит" << endl;
    }
    else{
        cout << "Не лежит" << endl;
    }
    return 0;
}
