#include <iostream>
#include <vector>

using namespace std;


int main(){
    setlocale(LC_CTYPE, "rus");
    vector<int> vec;
    int n;
    cout << "Введите число в 10-ой СС" << endl;
    cin >> n ;
    while (n > 0){
        vec.push_back(n%2);
        n = n/2;
    }
    cout << "" << endl;
    for (int i=vec.size()-1; i>=0; i--){
        cout << vec[i];
    }

    return 0;
}
