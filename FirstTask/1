#include <iostream>
#define N 10

using namespace std;

int main(){
  setlocale(LC_CTYPE, "rus");
  int arr[N];
  int max_sum = 0;
  int sum = 0;

  for(int i=0; i<N; ++i){
    cin >> arr[i];
  }

  for(int i=0; i<N; ++i){
    sum = max(sum+arr[i], 0);
    max_sum = max(sum, max_sum);
  }

  cout << "Макс сумма: " << max_sum << endl;
  return 0;
}
